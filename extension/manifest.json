{
  "manifest_version": 2,
  "name": "ChronicleSync",
  "version": "1.0.0",
  "description": "Secure browser history synchronization",
  "key": "chroniclesync",
  "permissions": [
    "storage",
    "history",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "settings.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
