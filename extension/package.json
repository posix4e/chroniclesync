{
  "name": "chroniclesync-extension",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "node server.js & xvfb-run --auto-servernum --server-args='-screen 0 1280x960x24' playwright test",
    "test:e2e:ui": "node server.js & xvfb-run --auto-servernum --server-args='-screen 0 1280x960x24' playwright test --ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/chrome": "^0.0.297",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.12.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.0.4",
    "babel-jest": "^29.7.0",
    "cors": "^2.8.5",
    "eslint": "^8.51.0",
    "express": "^4.21.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.12",
    "vitest": "^3.0.4"
  }
}
