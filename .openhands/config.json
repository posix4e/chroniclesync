{
  "project": "ChronicleSync",
  "description": "P2P Browser Extension for synchronizing browsing history",
  "testMode": "p2p",
  "testPorts": {
    "instance1": 12000,
    "instance2": 12001
  },
  "testCommands": {
    "all": "npm run test:e2e",
    "connection": "npm run test:connection",
    "sync": "npm run test:sync",
    "conflict": "npm run test:conflict",
    "performance": "npm run test:performance",
    "security": "npm run test:security",
    "debug": "npm run test:e2e:debug"
  },
  "setupScript": ".openhands/setup.sh",
  "testReportDir": "playwright-report",
  "testResultsDir": "test-results"
}