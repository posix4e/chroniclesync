{
  "manifest_version": 3,
  "name": "OpenHands Browser History Sync",
  "version": "1.0",
  "description": "Syncs browser history with OpenHands backend",
  "permissions": [
    "history",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "src/popup.html"
  },
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  }
}