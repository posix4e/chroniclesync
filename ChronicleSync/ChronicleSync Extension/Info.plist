<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>NSExtension</key>
    <dict>
        <key>NSExtensionPointIdentifier</key>
        <string>com.apple.Safari.web-extension</string>
        <key>NSExtensionPrincipalClass</key>
        <string>$(PRODUCT_MODULE_NAME).SafariWebExtensionHandler</string>
    </dict>
    <key>SFSafariWebExtensionConverterVersion</key>
    <string>14.0</string>
    <key>SFSafariContentScript</key>
    <array>
        <dict>
            <key>Script</key>
            <string>content-script.js</string>
            <key>Matches</key>
            <array>
                <string>http://*/*</string>
                <string>https://*/*</string>
            </array>
            <key>RunAt</key>
            <string>document_idle</string>
        </dict>
    </array>
    <key>SFSafariWebExtensionBundleIdentifiersToProcess</key>
    <array>
        <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    </array>
    <key>SFSafariBackgroundScript</key>
    <array>
        <string>background.js</string>
    </array>
    <key>SFSafariToolbarItem</key>
    <dict>
        <key>Action</key>
        <string>Command</string>
        <key>Identifier</key>
        <string>Button</string>
        <key>Image</key>
        <string>toolbar-icon.png</string>
        <key>Label</key>
        <string>ChronicleSync</string>
    </dict>
    <key>SFSafariContextMenu</key>
    <array>
        <dict>
            <key>Text</key>
            <string>ChronicleSync</string>
            <key>Command</key>
            <string>open-chroniclesync</string>
        </dict>
    </array>
    <key>SFSafariStyleSheet</key>
    <array>
        <dict>
            <key>Style</key>
            <string>styles.css</string>
            <key>Matches</key>
            <array>
                <string>http://*/*</string>
                <string>https://*/*</string>
            </array>
        </dict>
    </array>
</dict>
</plist>